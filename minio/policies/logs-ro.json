{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "ListLogsBucket",
      "Effect": "Allow",
      "Action": ["s3:ListBucket"],
      "Resource": ["arn:aws:s3:::logs"]
    },
    {
      "Sid": "ReadLogsObjects",
      "Effect": "Allow",
      "Action": ["s3:GetObject"],
      "Resource": ["arn:aws:s3:::logs/*"]
    }
  ]
}
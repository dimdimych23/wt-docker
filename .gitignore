/websoft/web-backend/platform/**
!/websoft/web-backend/platform/.gitkeep
/websoft/web-backend/components/**
!/websoft/web-backend/components/.gitkeep

# worker-backend
/websoft/worker-backend/platform/**
!/websoft/worker-backend/platform/.gitkeep
/websoft/worker-backend/components/**
!/websoft/worker-backend/components/.gitkeep

# Логи по нодам
/websoft/Logs/web-backend-1/**
!/websoft/Logs/web-backend-1/.gitkeep
/websoft/Logs/web-backend-2/**
!/websoft/Logs/web-backend-2/.gitkeep
/websoft/Logs/web-backend-init/**
!/websoft/Logs/web-backend-init/.gitkeep
/websoft/Logs/worker-backend/**
!/websoft/Logs/worker-backend/.gitkeep
